{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "plugin@typescript-eslint/recommend", "prettier", "prettier/@typescript-eslint/"],
    "overrides": [
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "max-len": [
            "error",
            {
              "code": 60,
              "tabWidth": 2,
              "ignoreComments": true, //"comments": 80
              "ignoreUrls": true,
              "ignoreStrings": false,
              "ignoreTemplateLiterals": true
            }
          ]
    }
}